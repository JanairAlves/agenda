unit uViewAgenda;

interface

uses
  Winapi.Windows, Winapi.Messages, System.SysUtils, System.Variants, System.Classes, Vcl.Graphics,
  Vcl.Controls, Vcl.Forms, Vcl.Dialogs, Vcl.ExtCtrls, Vcl.StdCtrls, uViewContato;

type
  TfViewAgenda = class(TForm)
    pnAgendaContatos: TPanel;
    pnCentral: TPanel;
    btAdicionarContato: TButton;
    procedure btAdicionarContatoClick(Sender: TObject);
    procedure FormCreate(Sender: TObject);
  private
    { Private declarations }
  public
    { Public declarations }
  end;

var
  fViewAgenda: TfViewAgenda;

implementation

{$R *.dfm}

procedure TfViewAgenda.btAdicionarContatoClick(Sender: TObject);
begin
  fViewContato.ShowModal;
end;

procedure TfViewAgenda.FormCreate(Sender: TObject);
begin
  ReportMemoryLeaksOnShutdown := True;
end;

end.
